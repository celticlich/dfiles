# ##################################################################
#
# Aliases
# ##################################################################
#
alias sk='seekkill'
alias reload='source ~/.zshrc && echo "ZSH Config Reloaded from ~/.zshrc"'
alias tkp='tkillpane'
alias ll='eza -alhg'
alias jj='mac'
alias arping='sudo arping'
alias gotosleep='pmset sleepnow'
alias yabedit='vi ~/.yabairc && source ~/.yabairc'
alias edit='vi ~/.zshrc && reload'
alias softwareupdate='sudo softwareupdate'
alias wireless='networksetup -setairportpower en1 '$1''
alias shr='netstat -nr'
alias extip='wget http://ipinfo.io/ip -qO -'
alias vi="vim"
alias psaux='ps aux | ccze -A -o nolookups'
alias brewup='brew update && brew upgrade'
alias pscpu='ps aux | sort -nr -k 3'
alias pscpu10='ps aux | sort -nr -k 3 | head -10'
alias psmem='ps aux | sort -nr -k 4'
alias psmem10='ps aux | sort -nr -k 4 | head -10'
alias busy='cat /dev/urandom | hexdump -C | grep "ca fe"'
alias b='~/scripts/b.rb'
alias mps='rm -rf ~/.config/mps-youtube/cache_py_3.9.1 && mpsyt'
alias randpw="( export LC_ALL=C; tr -dc _A-Z-a-z-0-9 < /dev/urandom | head -c 25 );echo"

###################################################################
#
# 20250429
###################################################################
#
alias fixyabai='echo "$(whoami) ALL=(root) NOPASSWD: sha256:$(shasum -a 256 $(which yabai) | cut -d " " -f 1) $(which yabai) --load-sa"'
alias workbox='ssh workbox'
alias yggdrasil='ssh yggdrasil'
alias fimbulwinter='ssh fimbulwinter'
alias draugr='ssh draugr'
alias methuselah='ssh -t pheron@methuselah "/usr/local/bin/zsh;"'
alias work-clone="git -c core.sshCommand='ssh -i ~/.ssh/id_rsa.tyler-github -o IdentitiesOnly=yes' clone"
alias home-clone="git -c core.sshCommand='ssh -i ~/.ssh/id_rsa.celticlich-github -o IdentitiesOnly=yes' clone"
